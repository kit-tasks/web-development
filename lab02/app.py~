from flask import Flask, send_file, redirect, url_for

app = Flask('lab01')

@app.route("/")
def index():
	return send_file("./static/index.html")
	
@app.route("/page")
def page():
	return send_file("./static/page.html")
	
@app.route("/list")
def list():
	return send_file("./static/list.html")

@app.errorhandler(404)
def unknown(e):
	return redirect(url_for(''))
